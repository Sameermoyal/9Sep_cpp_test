#include<bits/stdc++.h>
using namespace std;
struct Course {
    
    string name;
    string instructor;
    int grades[5];
    double s(){
        int k=0;
        for(int i=0;i<5;i++){
            k+=grades[i];
        }
        return (double)k/5;
    }
};


int main(){
    int n;
    cin>>n;
    
    Course obj[n];
    for(int i =0;i<n;i++){
        cout<<"enter course name"<<endl;
        cin>>obj[i].name;
        cout<<"enter course instructor"<<endl;
        cin>>obj[i].instructor;
        cout<<"enter student grades"<<endl;
        for(int j=0;j<5;j++){
            cin>>obj[i].grades[j];
        }
    }
    
    double maxi=0;
    string name;
    
    for (int i = 0; i <n; i++) {
        if(maxi<obj[i].s()){
            maxi=obj[i].s();
            name=obj[i].name;
        }
    }
    
    cout<<name;
    
    return 0;
}